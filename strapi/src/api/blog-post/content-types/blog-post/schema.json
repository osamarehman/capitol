{
  "kind": "collectionType",
  "collectionName": "blog",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog",
    "displayName": "Blog Posts",
    "description": "Migrated from Webflow collection: Blog Posts"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "searchable": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "searchable": true
    },
    "schemaMarkup": {
      "type": "text",
      "required": false
    },
    "customHtml": {
      "type": "text",
      "required": false
    },
    "customContent": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      },
      "required": false
    },
    "faqContent": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      },
      "required": false
    },
    "publishedDateShown": {
      "type": "datetime",
      "required": false
    },
    "postSummary": {
      "type": "text",
      "required": false
    },
    "authorTemp": {
      "type": "text",
      "required": false
    },
    "blogImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "blogImageAltTag": {
      "type": "text",
      "required": false
    },
    "blogSummary": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      },
      "required": false
    },
    "postBody": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      },
      "required": false
    },
    "calc": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "openGraphImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "weatherWidget": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "weatherWidgetLocation": {
      "type": "text",
      "required": false
    },
    "weatherWidgetRightParagraphText": {
      "type": "text",
      "required": false
    },
    "isCalcCommercial": {
      "type": "boolean",
      "required": false,
      "default": false
    }
  }
}